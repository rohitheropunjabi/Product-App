       <Table striped bordered hover size=\"sm\">\r\n                    <thead>\r\n                        <tr>\r\n                            <th>\r\n                                Title\r\n                            </th>\r\n                            <th>\r\n                                Description\r\n                            </th>\r\n                            <th>\r\n                                Price\r\n                            </th>\r\n                            <th>\r\n                                Brand\r\n                            </th>\r\n                            <th>\r\n                                Category\r\n                            </th>\r\n                            <th>\r\n                                Created by\r\n                            </th>\r\n                            <th>\r\n                                Date of Creation\r\n                            </th>\r\n                            <th>\r\n                                Actions\r\n                            </th>\r\n                        </tr>\r\n                    </thead>\r\n                    <tbody>\r\n                        {\r\n                            Products && Products.length > 0\r\n                            ?\r\n                            Products.map((item) =>{\r\n                                return(\r\n                                    <tr>\r\n                                        <td>\r\n                                            {item.title}\r\n                                        </td>\r\n                                        <td>\r\n                                            {item.description}\r\n                                        </td>\r\n                                        <td>\r\n                                            {item.price}\r\n                                        </td>\r\n                                        <td>\r\n                                            {item.brand}\r\n                                        </td>\r\n                                        <td>\r\n                                            {item.category}\r\n                                        </td>\r\n                                        <td>\r\n                                            {item.createdby}\r\n                                        </td>\r\n                                        <td>\r\n                                            {item.dateofcreation}\r\n                                        </td>\r\n                                        <td>\r\n                                            <Link to=\"/Edit\">\r\n                                                <Button onClick={() => handleEdit(\r\n                                                item.id, \r\n                                                item.title, \r\n                                                item.description, \r\n                                                item.price, \r\n                                                item.brand, \r\n                                                item.category, \r\n                                                item.createdby, \r\n                                                item.dateofcreation)}> EDIT\r\n                                                </Button>\r\n                                            </Link>\r\n                                            <Button onClick={() => handleDelete(item.id)}> DELETE</Button>\r\n                                        </td>\r\n                                    </tr>\r\n                                )\r\n                            })\r\n                            :\r\n                            \"No Data Available\"\r\n                        }\r\n                    </tbody>\r\n                </Table>\r\n                <br>\r\n                </br>\r\n                <Link to=\"/Create\">\r\n                    <Button size=\"lg\"> Add/Create a new Product  